<script>var _hmt=_hmt||[];(function(){var hm=document.createElement("script");hm.src="//hm.baidu.com/hm.js?6cd8a09ebfe4dfbfa5a405e22d5841a5";var s=document.getElementsByTagName("script")[0];s.parentNode.insertBefore(hm,s)})();</script></head>
<body>
<div class="container">
    <div class="main">

        <!--订单容器-->
        <div id="orders">



        </div>

        <!--退货原因-->
        <div class="block reason">
            <div class="title">退货原因</div>
            <div class="content">
                退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因退货原因
            </div>
        </div>
        <!--审核状态-->
        <div class="block status">
            <div class="title">审核状态</div>
            <div class="content">
                正在审核……
            </div>
        </div>
        <!--联系客服-->
        <div class="connect">
            <div class="btn btn-active">联系客服</div>
            <p class="text-primary"><span class="icon-phone"></span>400-616-1111</p>
            <p class="text-default">全国免费客服热线</p>
        </div>
    </div>
</div>
<div class="info-tip"></div>


<script type="text/template" id="tpl-item">
    <!--订单-->
    <div class="order-container">
        <div class="detail-link" data-id="<%= item.order_sn %>">
            <!--订单编号-->
            <div class="order-no">
                <span class="no-text">退货商品</span>
            </div>
            <!--商品列表-->
            <ul class="goods-container">
                <% _.each(item.product, function (product, i) { %>
                <li class="<%= i>0?'hidden':'' %>">
                    <img class="lazy img" src="http://h5.quxiu.me/assets/img/blank.png" dataimg="<%= Global.formatImgSrc(product.image.replace('http://api.local.ve.cn','http://img.ve.cn')) %>"  width="57" height="57" alt=""/>
                    <div class="good-information">
                        <p class="good-name"><%= product.name %></p>
                        <p class="good-number"><span class="icon-x"></span><span><%= product.number %></span></p>
                        <p class="good-type"><%= product.attr_name %></p>
                    </div>
                    <span class="good-price">￥<%= parseFloat(product.unit_price).toFixed(2) %></span>
                </li>
                <% }); %>
            </ul>
        </div>
        <!--显示或隐藏多件商品-->
        <% if(item.product.length>1){ %>
        <div class="goods-ctr show">查看全部<%= item.product.length %>件商品</div>
        <div class="goods-ctr hide hidden">收起</div>
        <% }; %>
    </div>

</script>
<script src="http://h5.quxiu.me/assets/js/3rd/zepto.min.js"></script>
<script src="http://h5.quxiu.me/min/?b=assets&f=lib/lib-h5log/h5log.js,lib/lib-storage/storage.js,lib/lib-login/login.js,3rd/underscore-min.js,lib/lib-httpurl/httpurl.js,lib/lib-lazyload/lazyload.js,lib/lib-infinitescroll/infinitescroll.js,lib/lib-notification/notification.js,lib/lib-api/api.js,lib/lib-gotop/gotop.js,ctrl/ctrl-topbar/topbar.js,ctrl/ctrl-bottombar/bottombar.js"></script>
<script src="assets/h5/return-track.js?t=20150104"></script>

</body>
</html>