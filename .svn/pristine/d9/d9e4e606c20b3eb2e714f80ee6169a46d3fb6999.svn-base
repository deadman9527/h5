<script type="text/template" id="tpl-userBaby">
    <!--用户头像部分-->
    <% _.each(userList, function (item,index) {
        var pic = item.babyType==3 ? "data:image/png;base64,":"data:image/png;base64,";
    %>
    <div class="personal-photo">
        <div class="personal-image" style="background-image: url(<%= pic%>);"><a href="babyinfo.html?type=1&name=<%= item.nickName%>&sex=<%= item.sex%>&babyId=<%= item.babyId%>&birthday=<%= item.birthday%>"><span class="arrow-right">修改</span></a></div>
        <div class="personal-text" id="username"><%= item.nickName%></div>
    </div>
    <% });%>
    <% _.each(itemList, function (item,index) {
        var pic=null,bir='生日';
        if(item.sex==1){//男童
            bir='生日';
            pic="data:image/png;base64,";
        }else if(item.sex==2){//女童
            bir='生日';
            pic="data:image/png;base64,";
        }else{//婴儿
            bir='预产期';
            pic="data:image/png;base64,";
        }
    %>
    <div class="item-list">
        <span class="bgimg"><img width="15" src="data:image/png;base64," /></span>
        <span class="bgimg2"><img width="15" src="data:image/png;base64," /></span>
        <ul>
            <li class="left">
                <img src="<%= pic %>" />
            </li>
            <li class="right">
                <p class="line-bottom"><%= item.name%><% if(item.age){%>（<%= item.age%>）<% }%></p>
                <p><%= bir%> <%= item.birthday%></p>
            </li>
        </ul>
        <div class="text-right"><a href="babyinfo.html?type=2&name=<%= item.name%>&sex=<%= item.sex%>&birthday=<%= item.birthday%>&babyId=<%= item.babyId%>">修改</a> &nbsp;<a href="javascript:;" delId="<%= item.babyId%>" class="babyDel">删除</a></div>
    </div>
    <% });%>
    <div class="item-list">
        <span class="bgimg"><img width="15" src="data:image/png;base64," /></span>
        <span class="bgimg2"><img width="15" src="data:image/png;base64," /></span>

        <p class="text-left" id="addbaby"><a href="babyinfo.html?type=2">+添加宝贝档案</a></p>
    </div>
</script>
